<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hrishi3331studio.template_informal">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".General.Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserAuth.Authentication" />
        <activity android:name=".General.MainActivity" />
        <activity android:name=".UserAuth.SignUp" />
        <activity android:name=".Support.ContactUs" />
        <activity android:name=".Support.Support"></activity>
    </application>

</manifest>